{
  "layer": "top",
  "exclusive": true,
  "position": "top",
  "fixed-center": true,
  "modules-left": [
    "custom/stream_status",
    "hyprland/workspaces"
  ],
  "modules-center": [
    "clock",
    "tray"
  ],
  "modules-right": [
    // "tray",
    // "network",
    "cpu",
    "memory",
    "pulseaudio",
    "battery#forest",
    "custom/lock"
  ],
